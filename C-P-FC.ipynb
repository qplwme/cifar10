  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "09be3c0a",
   "metadata": {},
   "outputs": [],
   "source": [
    "classes = ('plane', 'car', 'bird', 'cat', 'deer',\n",
    "           'dog', 'frog', 'horse', 'ship', 'truck')\n",
    "batch_size = 16\n",
    "epoch = 25"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "524d8f8e",
